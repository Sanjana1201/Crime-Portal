<div  style="margin-top: 10%;margin-left: 45%">
  <a href="police_station_add" class="btn btn-primary">Add Police Station</a>
</div>

<div style="padding:20px 50px 50px 50px;">
  <table class="table table-bordered">
   <thead class="thead-dark" style="background-color: black; color: white;">
     <tr>
       <th scope="col">Incharge Id</th>
       <th scope="col">Incharge Name</th>
       <th scope="col">Location of Police Station</th>
       <th scope="col">Click to delete</th>
    </tr>
  </thead>

<tbody style="background-color: white; color: black;">
  <%  requestedStation.forEach(function(policeStation){  %>
  <tr>

    <td><%= policeStation.inchargeId %></td>
    <td><%= policeStation.inchargeName %></td>
    <td><%= policeStation.location %>, <%= policeStation.state %></td>
    <td><a href="/delete/<%= policeStation.inchargeId %>" class="btn btn-danger btn-sm">Delete Police Station</a></td>
  <%    });  %>
  </tr>
</tbody>

</table>
</div>


