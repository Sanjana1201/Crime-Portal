<div style="padding:50px;margin-top:10px;">
    <table class="table table-bordered">
    <thead class="thead-dark" style="background-color: black; color: white;">
        <tr>
            <th scope="col">Complain Id</th>
            <th scope="col">Description</th>
            <th scope="col">Police Status</th>
            <th scope="col">Final Report</th>
            <th scope="col">Location of Crime</th>
        </tr>
    </thead>
     <tbody style="background-color: white; color: black;">
      <tr>
        <td><%= complainNumber %></td>
        <td><%= complainDescription %></td>
        <td><%= policeId %></td>
        <td><%= finalReport %></td>
        <td><%= complainLocation %>, <%= complaintState %></td>
      </tr>
     </tbody>
    </table>
</div>

<div style="padding:50px; margin-top:8px;">
    <table class="table table-bordered">
       <thead class="thead-dark" style="background-color: black; color: white;">
           <tr>
                <th scope="col">Date Of Update</th>
                <th scope="col">Case Update</th>
           </tr>
       </thead>
        <tbody style="background-color: white; color: black;">
        <tr>
            <td><%= updateDate %></td>
            <td><%= update %></td>
        </tr>
        </tbody>
    </table>
</div>

